<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br>
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name %>
  <br>
  <input type="text" name="song[note_contents][]">
  <input type="text" name="song[note_contents][]">
  <input type="text" name="song[note_contents][]">
  <input type="text" name="song[note_contents][]">

  <%= f.submit %>
<% end %>
