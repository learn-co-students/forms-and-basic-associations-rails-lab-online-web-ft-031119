<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br>
  <%= f.label :gerne_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <label>Notes</label>
  <input name="song[notes_ids][]" id="song_notes_1"><br>
  <input name="song[notes_ids][]" id="song_notes_2"><br><br>

  <%= f.submit %>
<% end %>


